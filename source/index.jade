extend /default


block body
    .row
        .col-xs-6.col-sm-4.col-xs-offset-3.col-sm-offset-4
            img.img-responsive(src='/assets/images/logo.png', alt='logo')

    .row
        each e in F.entities
            a.item-list.col-xs-12.col-sm-10.col-md-8.col-sm-offset-1.col-md-offset-2(href=e.properties.path[0].value + '/')= e.displayname
